{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I",
                "~/eigen-3.4.0",
                "-O3",
                "-std=c++11",
                "-lm",
                "${workspaceFolder}/src/Main.cpp",
                "${workspaceFolder}/src/Image.cpp",
                "-o",
                "${workspaceFolder}/out/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}